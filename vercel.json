{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
    "rewrites": [
      {
        "source": "/API/(.*)", 
        "destination": "/backend/$1" 
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }